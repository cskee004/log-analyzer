<div class="dashboard">
    <div class="button-bar">
        <%= button_to "Summary", summary_dashboard_path, method: :get, form: { data: { turbo_frame: "frame_panel" } }, disabled: true, id: "summary-button" %>
        <%= button_to "Graphs", graph_dashboard_path, method: :get, form: { data: { turbo_frame: "frame_panel" } }, disabled: true, id: "graph-button" %>
        <%= button_to "Tables", table_dashboard_path, method: :get, form: { data: { turbo_frame: "frame_panel" } }, disabled: true, id: "table-button" %>
    </div>

    <div class="frame">
        <turbo-frame id='frame_panel'>
            <%= render 'upload' %>
        </turbo-frame>
    </div>
</div>